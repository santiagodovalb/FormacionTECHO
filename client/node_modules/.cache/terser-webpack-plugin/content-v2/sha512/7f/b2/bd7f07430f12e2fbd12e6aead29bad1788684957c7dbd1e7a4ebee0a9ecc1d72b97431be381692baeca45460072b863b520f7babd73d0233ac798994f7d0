Col={{\n            span: 8,\n          }}\n          wrapperCol={{\n            span: 16,\n          }}\n          initialValues={{\n            remember: true,\n          }}\n          onValuesChange={(obj, obj2) => handleChange(obj, obj2)}\n          onFinish={handleSubmit}\n        >\n          <Form.Item label=\"Tipo de rol\" name=\"tipo\">\n            <Input.TextArea />\n          </Form.Item>\n\n          <Button\n            style={{ display: form.tipo ? \"block\" : \"none\" }}\n            htmlType=\"submit\"\n            type=\"submit\"\n          >\n            Crear rol\n          </Button>\n        </Form>\n      </div>\n      <Roles />\n    </div>\n  );\n}\n","import axios from \"axios\";\nimport React from \"react\";\nimport { useEffect } from \"react\";\nimport { useState } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport { Table } from \"antd\";\nimport \"./VolunteerEntregas.css\";\n\nexport default function VolunteerEntregas() {\n  const [entregas, setEntregas] = useState([]);\n  const user = useSelector((state) => state.user);\n\n  useEffect(() => {\n    axios\n      .get(`/api/entregas/user/${user.id}`)\n      .then((res) => res.data)\n      .then((entregas) => {\n        setEntregas(entregas);\n      });\n  }, [user]);\n\n  const dataSource = entregas.map((entrega) => {\n    return {\n      key: entrega.id,\n      bloque: entrega.bloque.titulo,\n      contenido: entrega.contenido,\n      estado: entrega.aprobado ? <p>Completado</p> : <p>Pendiente</p>,\n    };\n  });\n\n  const columns = [\n    {\n      title: \"Bloque\",\n      render: (record) => (\n        <React.Fragment>\n          {record.bloque}\n        </React.Fragment>\n      ),\n      responsive: [\"xs\"]\n    },\n    {\n      title: \"Estado\",\n      render: (record) => (\n        <React.Fragment>\n          {record.estado}\n        </React.Fragment>\n      ),\n      responsive: [\"xs\"]\n    },\n    {\n      title: \"Bloque\",\n      dataIndex: \"bloque\",\n   