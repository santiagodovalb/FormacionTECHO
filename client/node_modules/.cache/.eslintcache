a-bs-toggle=\"collapse\"\n            data-bs-target=\"#navbarSupportedContent\"\n            aria-controls=\"navbarSupportedContent\"\n            aria-expanded=\"false\"\n            aria-label=\"Toggle navigation\"\n          >\n            <span className=\"navbar-toggler-icon\"></span>\n          </button>\n          <div className=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n            <ul className=\"navbar-nav me-auto mb-2 mb-lg-0\">\n              <li className=\"nav-item\">\n                <Link to={`/user`}>\n                  <p className=\"nav-link\">Mi perfil</p>\n                </Link>\n              </li>\n              <Links />\n              <hr />\n              {user.rolId && user.rolId === 2 && (\n                <Link to=\"/gestor-password\">\n                  {\" \"}\n                  <button> Cambiar contraseña </button>\n                </Link>\n              )}\n              <Link to=\"/login\">\n                {\" \"}\n                <button onClick={handleLogout}> Cerrar sesión</button>\n              </Link>\n            </ul>\n          </div>\n        </div>\n      </nav>\n    </div>\n  );\n};\n\nexport default Navbar;\n","/home/santi/P5/TECHO/formacion-techo/client/src/containers/Login/style.js",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/UserProfile/UserProfile.jsx",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/Unauthorized/Unauthorized.jsx",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/Sedes/Sedes.jsx",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/Volunteer/VolunteerContent/VolunteerContent.jsx",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/Volunteer/VolunteerModuls/VolunteerModules.jsx",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/Volunteer/VolunteerEntregas/VolunteerEntregas.jsx",[],"/home/santi/P5/TECHO/formacion-techo/client/src/containers/Gestor/GestorPassword/GestorPassword.jsx",[],"/home/santi/P5/T